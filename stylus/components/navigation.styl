.nav
    width 100%
    height 50px
    ul,
    li
        position relative
        display inline-block
    ul
        display flex
        align-items center
        width 100%
        list-style none
        margin-left auto
        margin-right auto
        margin-top 10px
        margin-bottom 0
        padding-left 0
    li
        font-size 1.8rem
        color cat-white
        font-weight lighter
    a
        text-decoration none
        white-space nowrap
        font-size 1.8rem
        color cat-white
        font-weight lighter
    .logo
            margin-top 10px;
.fix
    fixed top left
.right
    ul
        justify-content flex-end
        margin-right 10px
    li
        margin-left 10px
